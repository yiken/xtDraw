<script type="text/javascript">
function sendPoints(arg)
{
	window.location = 'skp:createUnit@'+arg;
}

function exitsethole(name)
{
  window.location='skp:on_close@' + name;
}
function moveobj(data)
{
  window.location='skp:on_Move@' + data;
}
function changeobjsize(data)
{
  window.location='skp:on_ChangeSize@' + data;
}
function createunit(data)
{
  window.location='skp:on_createunit@' + data;
}
function backhoe(data)
{
  window.location='skp:on_backhoe@' + data;
}
function changedir(data)
{
  window.location='skp:on_changedir@' + data;
}

function Send_SelectTwoSize(sJson) {
	CLXBaseRun.DoCmdStr(1,sJson,200)
}

function Send_SelectOneSize(sJson) {
	CLXBaseRun.DoCmdStr(2,sJson,200)
}
   
      
</script>

<SCRIPT ID=ao1 LANGUAGE=javascript>
function CLXBaseRun_OnCustomStrEvent(EventID,Data,P) {
	if (EventID==0){
		exitsethole(Data);
	}else if (EventID==100){
		sendPoints(Data);
	}else if (EventID==201){
		moveobj(Data);
	}else if (EventID==202){
		changeobjsize(Data);
	}else if (EventID==203){
		createunit(Data);
	}else if (EventID==210){
		changedir(Data)
	}else if (EventID==211){
		backhoe(Data);
	}
return Data;
}
</SCRIPT>
<SCRIPT LANGUAGE=javascript FOR=CLXBaseRun EVENT=OnCustomStrEvent(EventID,Data,P)>
return CLXBaseRun_OnCustomStrEvent(EventID,Data,P)
</SCRIPT>


